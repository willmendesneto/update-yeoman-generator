{
  "name": "update-yeoman-generator",
  "version": "2.0.0",
  "description": "update-yeoman-generator - Update your Yeoman generator with style",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint .",
    "test": "mocha --recursive test/unit",
    "test:integration": "mocha --recursive test/integration",
    "tdd": "mocha -w",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "bin": {
    "update-yeoman-generator": "index.js"
  },
  "files": [
    "*.{js,md}",
    "LICENSE",
    "lib"
  ],
  "keywords": [
    "update",
    "yeoman",
    "generator",
    "update-yeoman-generator",
    "script"
  ],
  "author": "Will Mendes <willmendesneto@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.3.0",
    "ejs": "^3.1.6",
    "is-binary-buffer": "^2.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^0.5.1",
    "node-diff3": "^2.1.2",
    "node-fetch": "^2.6.1",
    "node-github-diff": "^3.0.0",
    "upath": "^1.0.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "changelog-verify": "^1.1.0",
    "eslint": "^6.1.0",
    "mocha": "^7.0.1",
    "version-changelog": "^3.1.0"
  }
}
